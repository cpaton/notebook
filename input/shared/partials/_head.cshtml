<head>
    <meta charset='utf-8'>
    <meta http-equiv="X-UA-Compatible" content="chrome=1">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    @if (!string.IsNullOrEmpty(Model.Get("title", string.Empty).ToString()))
    {
    <title>@Model["title"]</title>
    }
    else
    {
    <title>@Model["site.title"]</title>
    }

    @if (!string.IsNullOrEmpty(Model.Get("description", string.Empty).ToString()))
    {
    <meta name="description" content="@Model["description"]" />
    }
    else
    {
    <meta name="description" content="@Model["site.description"]" />
    }

    <link rel="canonical" href="@Model.String("CanonicalUrl").Replace("index.html", "")" />

    <!--<link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/stylesheets/stylesheet.css" media="screen">-->
    <!--<link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/stylesheets/github-dark.css" media="screen">-->
    <link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/css/main.css" media="screen">
    <link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/css/bootstrap.css" media="screen">
    <!--<link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/css/custom.css" media="screen">-->
    <!--<link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/css/bootstrap-theme.min.css" media="screen">-->
    
    <!--<link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/stylesheets/stylesheet.css" media="screen">
    <link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/stylesheets/github-dark.css" media="screen">-->
    
    <link rel="stylesheet" type="text/css" href="{{ site.baseurl }}/css/customisation.css" media="screen">
</head>