@{ Layout = "_default"; }

<div class="container">

    <div class="row">
        <div class="col-md-9">
            @if (Model.TryGetValue<string>("Title", out string title))
            {
                <h1>@title</h1>
                <hr/>
            }
            @RenderBody()
        </div>
        <div class="col-md-3">
            <nav id="toc" data-toggle="toc" data-spy="affix"></nav>
        </div>
    </div>
</div>